\frametitle{Properties of the cross product} 
\begin{eqnarray*}
  i \times j &=& k\\
  j \times k &=& i\\
  k \times i &=& j\\
  a \times b &=& -b\times a\\
  a\times (b+c) &=& a\times b + a \times c\\
  (sa) \times b &=& s(a \times b), s\in \mathbb{R}\\
a\times (b \times c) & \not =& (a\times b)\times c\\
|a\times b| &=& \sin(\theta)|a||b|
\end{eqnarray*}
